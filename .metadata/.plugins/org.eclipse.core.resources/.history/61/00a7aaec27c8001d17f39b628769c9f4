package com.pojobrans;

class Pojo {
	private String text;
	private int number;

	public String toStirng() {
		return text + ":" + number;
	}
}
	public class SpringBeansVsJavaBeans {

		public static void main(String[] args) {
			// TODO Auto-generated method stub
			Pojo pojo = new Pojo();
			System.out.println(pojo);
		}
	}


